{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": [
    "lastName",
    "numBags"
  ],
  "properties": {
    "lastName": {
      "$id": "#/properties/lastName",
      "type": "string",
      "title": "The Lastname Schema",
      "default": "",
      "examples": [
        "Smith"
      ],
      "pattern": "^(.*)$"
    },
    "numBags": {
      "$id": "#/properties/numBags",
      "type": "integer",
      "title": "The Numbags Schema",
      "default": 0,
      "examples": [
        2
      ]
    }
  }
}